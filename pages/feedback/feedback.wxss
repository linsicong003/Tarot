/* pages/feedback/feedback.wxss */
image{
  width: 100%;
  height: 100%;
}
.content_wrap{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-family: "黑体";
}

.text_wrap{
  color: #fff;
  font-size: 36rpx;
  margin-top: 166rpx;
}

.input_wrap{
  margin-top: 68rpx;
  width: 505rpx;
  height: 320rpx;
  border-radius: 5rpx;
}

.input{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  border-radius: 8rpx;
  box-shadow: 5rpx #000;
  padding: 33rpx 26rpx;
  background-color: #fff;
  font-size: 32rpx;
  letter-spacing: 5rpx;
  word-break: keep-all;
}

.submit{
  width: 220rpx;
  height: 90rpx;
  position: relative;
  margin-top: 55rpx;
}

.submit_bg{
  width: 100%;
  height: 100%;
}

.submit_text{
  font-size: 30rpx;
  color: #6f24a0;
  width: 100%;
  height: 100%;
  position: absolute;
  top:0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center; 
}